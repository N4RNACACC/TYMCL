<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:themes="clr-namespace:ShadUI.Themes;assembly=ShadUI"
             x:Class="TYMCL.App"
             RequestedThemeVariant="Default">
             <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->

  <Application.Styles>
    <themes:ShadTheme />
  </Application.Styles>
  <Application.Resources>
    <FontFamily x:Key="IconFonts_Solid">
      avares://TYMCL/Assets/Fonts/FontAwesome6Free-Solid-400.otf#Font Awesome 6 Free Solid
    </FontFamily>
    <FontFamily x:Key="IconFonts_Regular">
      avares://TYMCL/Assets/Fonts/FontAwesome6Brands-Regular-400.otf#Font Awesome 6 Brands Regular
    </FontFamily>
    
  </Application.Resources>
</Application>